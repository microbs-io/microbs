{
	"job": "ecommerce-web-gateway",
	"target": "http://web-gateway.default.svc.cluster.local/",
	"enabled": true,
	"labels": [],
	"probes": [],
	"timeout": 3000,
	"frequency": 10000,
	"alertSensitivity": "high",
	"settings": {
		"http": {
			"method": "GET",
